@model Session
@{
    ViewData["Title"] = "Sessies - Leerkracht";
}

<h2>@ViewData["Title"]</h2>

<h4>
    Bevestig dat u sessie: @ViewData["Name"] wilt activeren...
</h4>
<form asp-action="Activate" method="post">
    <div>
        <button class="btn btn-primary" type="submit">Activeer</button>
        <a asp-action="Index" class="btn btn-default">Cancel</a>
    </div>
</form>